{
  "name": "seizuremate-expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "release": "standard-version",
    "prepare": "husky install",
    "commit": "cz",
    "test:prepush": "npm test --if-present",
    "build:prepush": "npm run build --if-present"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.18.3",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@rn-primitives/avatar": "^1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@shopify/flash-list": "2.0.2",
    "@supabase/supabase-js": "^2.80.0",
    "@tanstack/react-query": "^5.90.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "expo": "~54.0.22",
    "expo-apple-authentication": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "^16.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-store-review": "~9.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "moti": "^0.30.0",
    "nativewind": "^4.2.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.0",
    "react-native": "0.81.5",
    "react-native-bottom-tabs": "^1.0.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-purchases": "^9.6.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-uitextview": "^2.1.0-rc.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "rn-icon-mapper": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "standard-version": "^9.5.0",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{md,json,css,scss,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-git"
    },
    "cz-git": {
      "scopes": [
        "setup",
        "ui",
        "core",
        "api",
        "docs",
        "build"
      ],
      "enableMultipleScopes": true
    }
  },
  "private": true
}
